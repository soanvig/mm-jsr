{
  "name": "mm-jsr",
  "version": "3.0.2",
  "author": "Mateusz Koteja <soanvig@gmail.com>",
  "description": "Pure JS range input solution",
  "license": "LGPLv3",
  "main": "build/index.js",
  "types": "build/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/soanvig/mm-jsr.git"
  },
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "watch": "rollup --watch --config rollup.config.js",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.test.json ava --verbose",
    "docs": "typedoc ./src/api.ts",
    "manual-test": "yarn build && yarn serve -l 3000 ."
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "13.2.1",
    "@rollup/plugin-typescript": "8.3.2",
    "@types/node": "17.0.24",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "@zerollup/ts-transform-paths": "1.7.18",
    "ava": " 3.15.0",
    "browser-env": "3.3.0",
    "cross-env": "7.0.3",
    "eslint": "8.13.0",
    "eslint-plugin-import-quotes": "0.0.1",
    "rollup": "2.79.2",
    "rollup-plugin-terser": "7.0.2",
    "serve": "13.0.2",
    "ts-node": "10.7.0",
    "tslib": "2.3.1",
    "ttypescript": "1.5.13",
    "typedoc": "0.22.15",
    "typescript": "4.4.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "./tests/browser-env.js"
    ]
  }
}
