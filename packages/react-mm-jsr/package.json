{
  "name": "react-mm-jsr",
  "version": "2.0.4",
  "author": "Mateusz Koteja <soanvig@gmail.com>",
  "description": "React adapter to pure JS range input solution",
  "license": "LGPLv3",
  "main": "build/index.js",
  "types": "build/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/soanvig/mm-jsr.git"
  },
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "watch": "rollup --watch --config rollup.config.js",
    "play": "ts-node -P tsconfig.test.json ./src/index.ts",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.test.json ava --verbose"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-typescript": "8.3.0",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "@zerollup/ts-transform-paths": "1.7.18",
    "browser-env": "3.3.0",
    "cross-env": "7.0.3",
    "eslint": "8.9.0",
    "eslint-plugin-import-quotes": "0.0.1",
    "mm-jsr": "2.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "2.67.3",
    "rollup-plugin-terser": "7.0.2",
    "ts-node": "10.5.0",
    "ttypescript": "1.5.13",
    "typescript": "4.4.3"
  },
  "peerDependencies": {
    "mm-jsr": "~2.0.4",
    "react": "17.0.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "./tests/browser-env.js"
    ]
  }
}
